{
  "name": "reactnativegetstarted",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env react-native run-android",
    "ios": "ENVFILE=.env react-native run-ios",
    "storybook:android": "yarn prestorybook && ENVFILE=.env.storybook react-native run-android && sb-rn-watcher",
    "storybook:ios": "yarn prestorybook && ENVFILE=.env.storybook react-native run-ios && sb-rn-watcher",
    "start": "react-native start",
    "test": "jest --config test/jest.config.js --coverage --silent=false -u -w 1",
    "pods": "cd ios && npx pod-install && cd ..",
    "gradlew": "cd android && ./gradlew clean && cd ..",
    "coverage": "open test/coverage/lcov-report/index.html",
    "lint": "eslint .",
    "storybook": "start-storybook -p 7007",
    "prestorybook": "sb-rn-get-stories",
    "build-storybook": "build-storybook",
    "reset": "watchman watch-del-all && rm -f yarn.lock && rm -rf node_modules && yarn && yarn start --reset-cache"
  },
  "dependencies": {
    "@emotion/native": "^11.0.0",
    "@emotion/react": "^11.4.1",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-native-community/datetimepicker": "^5.1.0",
    "@react-native-community/slider": "^4.2.1",
    "@react-navigation/devtools": "^6.0.1",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@react-navigation/stack": "^6.0.7",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-controls": "^6.4.19",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.5",
    "@storybook/addon-ondevice-notes": "^6.0.1-beta.5",
    "@storybook/react-native": "^6.0.1-beta.5",
    "apisauce": "^2.1.1",
    "prop-types": "^15.7.2",
    "react": "18.1.0",
    "react-native": "0.70.0",
    "react-native-config": "^1.4.3",
    "react-native-device-info": "^8.3.3",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-reanimated": "^3.0.0-rc.2",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-svg": "^12.1.1",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-action-watch": "^1.0.6",
    "redux-saga": "^1.1.3",
    "reduxsauce": "^1.2.1",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@emotion/jest": "^11.3.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@testing-library/jest-native": "^4.0.2",
    "@testing-library/react-native": "^7.2.0",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-html-reporter": "^3.4.1",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-dom": "17.0.2",
    "react-native-codegen": "^0.0.7",
    "react-native-storybook-loader": "^2.0.4",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "^17.0.2",
    "redux-saga-test-plan": "^4.0.3",
    "typescript": "^4.3.5"
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "packageManager": "yarn@1.22.1"
}
